{
  "name": "ltd-beget/sphinx-configurator",
  "type": "library",
  "description": "Php library for parsing and editing sphinx.conf files.",
  "keywords": [
    "sphinx"
  ],
  "authors": [
    {
      "name": "Viskov Sergey",
      "homepage": "https://github.com/voksiv"
    }
  ],
  "require": {
    "php": ">=7.2",
    "symfony/yaml" : "^3.4.0 || ^4.0.0",
    "marc-mabe/php-enum" : ">=2.2",
    "ltd-beget/sphinx-configuration-tokenizer":"^2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.3",
    "tideways/profiler": "2.0.*"
  },
  "autoload": {
    "psr-0": {
      "LTDBeget\\sphinx": "src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "LTDBeget\\dev": "src"
    }
  }
}
