{
  "name": "ltd-beget/sphinx-configurator",
  "type": "library",
  "description": "Php library for parsing and editing sphinx.conf files.",
  "keywords": [
    "sphinx"
  ],
  "authors": [
    {
      "name": "Viskov Sergey",
      "homepage": "https://github.com/voksiv"
    }
  ],
  "require": {
    "php": ">=7.2",
    "symfony/yaml" : "3.4.*",
    "marc-mabe/php-enum" : "4.7.*",
    "ltd-beget/sphinx-configuration-tokenizer":"~1.0.3 | dev-BH-15288"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5.3",
    "tideways/profiler": "2.0.*"
  },
  "autoload": {
    "psr-0": {
      "LTDBeget\\sphinx": "src"
    }
  },
  "minimum-stability" : "dev",
  "prefer-stable" : true,
  "autoload-dev": {
    "psr-0": {
      "LTDBeget\\dev": "src"
    }
  }
}
