{
  "name": "ltd-beget/sphinx-configurator",
  "type": "library",
  "description": "Php library for parsing and editing sphinx.conf files.",
  "keywords": [
    "sphinx"
  ],
  "authors": [
    {
      "name": "Viskov Sergey",
      "homepage": "https://github.com/voksiv"
    }
  ],
  "require": {
    "php": ">=7.0",
    "respect/validation": "1.0.*",
    "mattketmo/camel": "1.1.*",
    "symfony/yaml" : "3.0.*",
    "marc-mabe/php-enum" : "2.2.*"
  },
  "require-dev": {
    "theseer/phpdox": "0.8.*"
  },
  "autoload": {
    "psr-0": {
      "LTDBeget\\sphinx": "src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "LTDBeget\\dev": "src"
    }
  }
}
